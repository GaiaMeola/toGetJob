+package org.example.togetjob.view.gui.controllergrafico;
+
+import javafx.fxml.FXML;
+import javafx.scene.control.Button;
+import javafx.scene.control.TitledPane;
+import javafx.scene.input.MouseEvent;
+import org.example.togetjob.printer.Printer;
+import org.example.togetjob.view.boundary.SendAJobApplicationStudentBoundary;
+import org.example.togetjob.view.gui.GUIContext;
+import org.example.togetjob.view.gui.concretestate.HomeState;
+
+public class HomeStudentController {
+
+    @FXML
+    private Button viewProfileButton;
+    @FXML
+    private Button notificationsButton;
+    @FXML
+    private Button logoutButton;
+    @FXML
+    private Button searchForAJobButton;
+    @FXML
+    private TitledPane pendingApplicationsTitlePane;
+    @FXML
+    private TitledPane acceptedApplicationsTitlePane;
+    @FXML
+    private TitledPane rejectedApplicationsTitlePane;
+
+    private GUIContext context;
+
+
+    public HomeStudentController() {
+       /**/
+    }
+
+    public void setContext(GUIContext context) {
+        this.context = context;
+    }
+
+    @FXML
+    public void handleViewProfile() {
+        Printer.print("Navigating to View Profile...");
+    }
+
+    @FXML
+    public void handleNotifications() {
+        Printer.print("Opening notifications...");
+    }
+
+    @FXML
+    public void handleLogout() {
+        Printer.print("Logging out...");
+        context.setState(new HomeState(context));
+        context.showMenu();
+    }
+
+    @FXML
+    public void handleSearchForJob() {
+        Printer.print("Searching for job announcements...");
+    }
+
+    @FXML
+    public void handlePendingApplications(MouseEvent event) {
+        Printer.print("Opening pending job applications...");
+    }
+
+    @FXML
+    public void handleAcceptedApplications(MouseEvent event) {
+        Printer.print("Opening accepted job applications...");
+    }
+
+    @FXML
+    public void handleRejectedApplications(MouseEvent event) {
+        Printer.print("Opening rejected job applications...");
+    }
+}
