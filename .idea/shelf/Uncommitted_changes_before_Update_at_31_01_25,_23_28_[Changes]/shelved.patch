+package it.model.dao.concreteobjects;
+
+import com.google.gson.*;
+import it.model.entity.User;
+import it.model.entity.Student;
+import it.model.entity.Recruiter;
+
+import java.lang.reflect.Type;
+
+public class UserTypeAdapter implements JsonDeserializer<User> {
+    @Override
+    public User deserialize(JsonElement json, Type typeOfT, JsonDeserializationContext context) throws JsonParseException {
+        JsonObject jsonObject = json.getAsJsonObject();
+        String role = jsonObject.get("role").getAsString();
+
+        // Deserializing based on role
+        if ("STUDENT".equalsIgnoreCase(role)) {
+            return new Gson().fromJson(json, Student.class);
+        } else if ("RECRUITER".equalsIgnoreCase(role)) {
+            return new Gson().fromJson(json, Recruiter.class);
+        } else {
+            throw new JsonParseException("Unknown role: " + role);
+        }
+    }
+}
